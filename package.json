{
  "name": "agencia-compromisso-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "~4.10.1",
    "@material-ui/icons": "~4.9.1",
    "@material-ui/lab": "~4.0.0-alpha.55",
    "axios": "~0.19.2",
    "dayjs": "~1.8.28",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.2",
    "firebase": "~7.15.0",
    "formik": "~2.1.4",
    "prop-types": "~15.7.2",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-router-dom": "~5.2.0",
    "react-scripts": "~3.4.1",
    "react-text-mask": "~5.4.3",
    "reactfire": "~2.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "~5.9.0",
    "@testing-library/react": "~10.2.0",
    "@testing-library/react-hooks": "~3.3.0",
    "codecov": "~3.8.3",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.2",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-prettier": "~3.1.3",
    "husky": "~4.2.5",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "~2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src",
    "fix:test": "npm run lint:fix && npm test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hackathon-da-diversidade/agencia-compromisso-web.git"
  },
  "bugs": {
    "url": "https://github.com/hackathon-da-diversidade/agencia-compromisso-web/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/api/**.js",
      "./src/components/**/**.js"
    ]
  }
}
