  "dependencies": {
    "@material-ui/core": "^4.9.3",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.43",
    "axios": "^0.19.0",
    "dayjs": "^1.8.18",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "firebase": "^7.14.5",
    "formik": "^1.5.8",
